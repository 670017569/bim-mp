.iconfont {
    font-family: "iconfont" !important;
    font-size: 45rpx;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-shouhuodizhi:before {
    content: "\e611";
    color: rgb(80, 190, 157);
}

.icon-huodong:before {
    content: "\e600";
    color: rgb(79, 207, 138);
}

.icon-dingdan:before {
    content: "\e601";
    color: rgb(17, 142, 233);
}

.icon-yue:before {
    content: "\e68a";
    color: rgb(255, 176, 62);
}

page {
    background-color: #f6f6f6;
}

.base_msg {
    height: 490rpx;
}

.amountBg {
    height: 250rpx;
    width: 100%;
    display: inline-block;
    background-color: #3399fd;
    border-radius: 0 0 30rpx 30rpx;
}

.user_box {
    width: 95%;
    margin: 0 auto;
    transform: translateY(-45%);
    background-color: #ffffff;
    border-radius: 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.user_msg {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 50rpx 0;
    overflow: hidden;
    width: 100%;
}

.user_img {
    width: 120rpx;
    height: 120rpx;
    display: block;
    border-radius: 50%;
    overflow: hidden;
}

.account {
    width: 43%;
    color: #000;
    margin-left: 18.1rpx;
}

.nav {
    width: 253.4rpx;
    height: 72.4rpx;
    margin: 18.1rpx auto;
    text-decoration: none;
    font: bold 1.5em 'Trebuchet MS', Arial, Helvetica;
    display: inline-block;
    text-align: center;

    line-height: 72.4rpx;
    color: #000;
    border: 1.81rpx solid #9c9c9c;
    border: 1.81rpx solid rgba(0, 0, 0, 0.3);
    text-shadow: 0 1.81rpx 0 rgba(0, 0, 0, 0.4);
    box-shadow: 0 0 0.05em rgba(0, 0, 0, 0.4);
    -moz-box-shadow: 0 0 .05em rgba(0, 0, 0, 0.4);
    -webkit-box-shadow: 0 0 0.05em rgba(0, 0, 0, 0.4);
}

.nav_font1 {
    font-size: 27.15rpx;
}

.nav_font2 {
    font-size: 21.72rpx;
}

.order {
    width: 94%;
    height: 126.7rpx;
    display: flex;
    flex-direction: row;
    background-color: #ffffff;
    border-radius: 9.05rpx;
    text-align: center;
    align-items: center;
    top: 162.9rpx;
}

.desc {
    width: 24%;
    font-size: 28rpx;
}

.desc::before {
    content: '';
    display: block;
    width: 1rpx;
    height: 50rpx;
    background-color: #cccccc;
    float: right;
    transform: translateY(30%);
}

.desc:last-child::before {
    content: '';
    display: none;
}

.clear {
    padding-top: 126.7rpx;
}

.content {
    width: 94%;
    margin: 0 auto;
    border-radius: 20rpx;
    overflow: hidden;
    display: flex;
    align-items: center;
    justify-content: center;
}

.item {
    padding: 15rpx 0;
    width: 25%;
    background-color: #ffffff;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}

.item:active {
    background-color: #ddd
}

.icon {
    width: 90.5rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.icon icon {
    margin-bottom: 11rpx;
}

.itemName {
    font-size: 25rpx;
}

.line {
    height: 18.1rpx;
}

.hr {
    height: 1.81rpx;
    background-color: #cccccc;
    opacity: 0.2;
}

.remark {
    font-weight: normal;
    margin-top: 9.05rpx;
}

.right {
    width: 40%;
    text-align: right;
}

.opr {
    color: #5495E6;
    font-size: 23.53rpx;
    font-weight: bold;
    margin-right: 18.1rpx;
}

.integral {
    display: inline-block;
    font-size: 30rpx;
    background-color: #e4f3ff;
    border-radius: 20rpx;
    color: #5495E6;
    font-weight: 600;
    transform: scale(0.8) translateX(-15rpx);
    padding: 5rpx 20rpx;
}

.order {
    font-size: 32rpx;
    font-family: Arial, Helvetica, sans-serif;
    font-weight: 500;
}

.number {
    font-weight: bold;
    font-size: 36rpx;
}