.pages{
  width: 100%;
  height: 1600rpx;
}
.fenge{
  width: 100%;
  border-bottom: 3rpx #dedede solid;
}
.clear{
  clear: both;
}



/* 学堂 */
.xuetang_container{
  width: 100%;
  height: 37%;
}
.container_top{
  width: 100%;
  background-color: whitesmoke;
  float: left;
  border-bottom: 2rpx #dedede solid;
  padding: 10rpx 0;
}
.xuetang_hot{
  height: 100%;
  width: 35%;
  padding-left: 20rpx;
  float: left;
  display: inline-block;
  font-weight:700;
  font-family:SimHei;
}
.xuetang_more{
  width: 25%;
  float: right;
  display: inline-block;
  font-size:30.76rpx;
  color:#747474;
  font-family:SimHei;
}

/* 案例 */
.cases_main{
  display: flex;
  width: 100%;
  height: 60%;
}
.cases_main_img{
  flex: 1;
  margin: 0;
  padding: 0;
  display: inline-block;
  height: 100%;
}

.cases_foot{
  width: 100%;
  height: 20%;
}
.cases_foot_text1{
  display: block;
  width: 100%;
  height: 55%;
  margin-top: 5rpx;
  margin-left: 20rpx;
  font-size:36.12rpx;

}
.cases_foot_text2{
  display: block;
  width: 95%;
  height: 45%;
  margin-left: 20rpx;
  color:#747474;
  font-size:32.42rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  word-wrap: nowrap;
}
/* 案例 */


/* 软件 */
.software_main{
  width: 250rpx;
  height: 250rpx;
  padding: 20rpx;
  float: left;
}
.software_main_img{
  height: 80%;
}
.software_main_content{
  font-size: 30rpx;
  text-align: center;
  padding-top: 10rpx;
  /* color: #fff; */
}

.software_info{
  float: left;
  display:block;
  height: 45%;
  width: 100%;
  padding-left: 10rpx;
  color:#747474;
  font-size:32.42rpx;
}
/* 软件 */

/* 课程 */
.class_main{
  float: left;
  display:flex;
  width: 100%;
  height: 45%;
}
.class_main_info{
  flex: 1;
  display: flex;
  width: 49%;
  height: 100%;
  flex-direction:column;
}
.class_main_info_img{
  flex: 1;
  width: 80%;
  height: 65%;
  margin: 0 20rpx;
}
.class_title{
  margin-left: 20rpx;
  font-size:36.12rpx;
}
.class_content{
  color:#747474;
  font-size:32.42rpx;
  margin-left:20rpx;
  margin-right: 40rpx;
}
/* 课程 */
/* 学堂结束 */

/* 话题 */
.searchInput{
  display: flex;
  width: 82%;
  height: 80rpx;
  margin-top: 10rpx;
  margin-left: 70rpx;
  margin-right: 20rpx;
  border-radius: 40rpx;
  
}
.searchInput_navigator{
  display: flex;
  height: 100%;
  width: 100%;
  justify-content: center;
  align-items: center;
  background-color: #5494E6;
  border-radius: 40rpx;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  font-size: 32rpx;
  color:#fff;
}
.huati_main{
  padding: 24.3rpx 36.2rpx;
  border-bottom: 1.81rpx solid #ddd;
}
.img{
    float: left;
}
.portrait{
  width: 88.69rpx;
  height: 85.07rpx;
  border-radius: 25px;
}
.nickName{
  float: left;
  font-size: 36.2rpx;
  height: 90.5rpx;
  margin-left: 36.2rpx;
}
.huatiItem{
  border-bottom: 1px solid #ddd;
  padding-bottom: 10px;
  margin-bottom: 5px
}
.title{
  min-height: 70rpx;
  font-size: 45rpx;
  line-height: 70rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;

}
.van-cell-text{
  width: 400rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.bottomText{
  margin-top: 50rpx;
  color: #999;
  height: 40rpx;
  line-height: 40rpx;
}
.topicCms,.topicClick{
  float: right;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  margin-right: 50rpx;
}
/* 话题结束 */




/* 标准 */
.standard{
  display: flex;
  flex-direction: row;
  background-color: rgb(245, 245, 244);
  padding: 8rpx 0;
  height: 1500rpx;
}
.standard_province_container{
  flex: 1.4;
  height: 100%;
  background-color:var(--cell-background-color,#fff);
}
.standard_content{
  flex: 5;
  height: 100%;
}
.standard_province{
  width: 100%;
  text-align: center;
  height: 60rpx;
  line-height: 57rpx;
  border-bottom: 1rpx solid #eee;
  margin-top: 10rpx;
}
.standard_province_hover{
  width: 100%;
  text-align: center;
  height: 60rpx;
  line-height: 57rpx;
  border-bottom: 1rpx solid #eee;
  margin-top: 10rpx;
  color: #5494E6;
}
.standard_view{
  margin-top: 2%;
  margin-bottom: 2%;
  margin-left: 2%;
  margin-right: 5%;
  background-color: #fff;
  white-space: normal;
}
.standard_time{
  margin-left: 65%;
  margin-right: 4rpx;
  margin-top: 5rpx;
  margin-bottom: 12rpx;
  font-size: x-small;
  color: rgb(181, 181, 177);
}

/* 标准结束 */





/* 头条开始 */
.lunbo{
  width: 100%;
  text-align: center;
}
.news_tab{
  display: flex;
  width: 100%;
  height: 100rpx;
}
.tab_hot_news{
  display: flex;
  flex: 1;
  height: 100%;
  width: 50%;
  justify-content: center;
  align-items: center;
  border-bottom:4rpx solid #5494E6;
  color: #5494E6;
  font-size: 33rpx;
}
.tab_all_news{
  display: flex;
  flex: 1;
  height: 100%;
  width: 50%;
  justify-content: center;
  align-items: center;
  font-size: 33rpx;
}
.news{
  margin-top: 15rpx;
  margin-right: 20rpx;
  margin-left: 20rpx;
  display: flex;
  width: 95%;
  height: 220rpx;
}
.news_info{
  display: inline-block;
  flex: 1;
  height: 95%;
  width: 50%;
  padding: 10rpx 20rpx 0 10rpx;
  position: relative;
}
.newsTitle{
  padding: 5rpx 0;
  width: 95%;
  margin-top: 5rpx;
  font-size: 34rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display: inline-block;
}
.newsContent{
  font-size: 25rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.news_other_info{
  margin-top: 10rpx;
  position: absolute;
  bottom: 0;
}
.news_other_info text{
  display: inline-block;
  margin: 10rpx 25rpx 0 0;
  font-size: 25rpx;
  color: #999898;
}
.news_img{
  height: 100%;
  width: 35%;
  display: inline-block;
}
.news_fenge{
  border-bottom: 2rpx #dedede solid;
  width: 95%;
  margin-left: 20rpx;
  margin-top: 5rpx;
}


/* 收费标准 */
.biaozhun{
  height: 100%;
  width: 85%;
  padding-left: 20rpx;
  float: left;
  display: inline-block;
  font-weight:700;
  font-family:SimHei;
}
.biaozhunTitle{
  width: 100%;
  height: 100%;
  margin-top: 15rpx;
  font-size: 29rpx;
  margin-left: 5rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: normal;
  display: inline-block;
}

.province{
  font-size: 30rpx;
  margin-left: 15rpx;
}

.biaozhun_test{
  width: 100%;
  height: 400rpx;
  display: inline-block;
  border-bottom: 3rpx #dedede solid;
 
}
.left_menu{
  display: inline-block;
  float: left;
  height: 100%;
  width: 18.5%;
  border-right: 3rpx #dedede solid;
}
.right_content{
  display: inline-block;
  padding-left: 15rpx;
  float: left;
  height: 100%;
  width: 78%;
}

.menu_item{
  padding: 12% 14%;
  height: 70rpx;
  width: 100%;
  display: inline-block;
  justify-content: center;
  align-items: center;
  font-size: 30.5rpx;
  background-color: whitesmoke;
}
.active{
  color:#5494E6;
  border-left: 5rpx solid currentColor;
  background-color: white;
}
/* 头条结束 */