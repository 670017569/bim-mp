<view class="xuetang_container" wx:for="{{sample}}" wx:key="key" wx:for-item="item">
  <navigator url="/pages/home/toutiao/news_detail/news_detail?url={{item.url}}" class='cases_main'>
      <view style="width:100%;height:100%"><van-image width="100%" height="100%" src="{{item.thumb}}" fit="contain" /></view>
  </navigator>
  <view class='cases_foot'>
    <text class='cases_foot_text1'>{{item.title}}</text>
    <text class='cases_foot_text2'>{{item.subtitle}}</text>
  </view>
  <view class='fenge'></view>
</view>